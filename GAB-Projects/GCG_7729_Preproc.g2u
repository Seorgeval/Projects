F.ODBC.Connection!con.OpenCompanyConnection

F.Data.DataTable.CreateFromSQL("dtOpenJobs", "con", "Select distinct JOB + '-' + SUFFIX as JS from JOB_HEADER Where DATE_CLOSED = '000000'", True)
F.Data.Dictionary.CreateFromSQL("dCustomer", "con", "Select distinct rtrim(Customer)Customer, rtrim(NAME_CUSTOMER)NAME_CUSTOMER from CUSTOMER_MASTER where REC = '1' order by NAME_CUSTOMER asc", True)
F.Data.Dictionary.CreateFromSQL("dPL", "con", "Select distinct rtrim(PROD_LINE)PROD_LINE, rtrim(PL_NAME)PL_NAME from PL_MASTER order by PL_NAME asc", True)
F.Data.Dictionary.CreateFromSQL("dSales", "con", "Select distinct rtrim(ID)ID, rtrim(NAME)NAME from SALESPERSONS order by NAME asc", True)
F.Data.Dictionary.CreateFromSQL("dWC", "con", "Select distinct rtrim(MACHINE)MACHINE, rtrim(WC_NAME)WC_NAME from WORKCENTERS order by WC_NAME asc", True)

F.ODBC.Connection!con.Close

#Region "WO"
F.Data.DataTable.CreateFromString("dtWo", "", "WO_Start*!*WO_End", "String*!*String", "*!*", "@!@", True)

#End Region

#Region "Customer"
F.Data.DataTable.CreateFromString("dtCust", "", "Cust_Start*!*Cust_End", "String*!*String", "*!*", "@!@", True)

#End Region

#Region "Due_Date"
F.Data.DataTable.CreateFromString("dtDueDate", 1-1-1900, "Due_Date_Start*!*Due_Date_End", "Date*!*Date", "*!*", "@!@", True)

#End Region 

#Region "PL"
F.Data.DataTable.CreateFromString("dtPL", "", "PL_Start*!*PL_End", "String*!*String", "*!*", "@!@", True)

#End Region

#Region "SalesPerson"
F.Data.DataTable.CreateFromString("dtSales", "", "SalesPerson_Start*!*SalesPerson_End", "String*!*String", "*!*", "@!@", True)

#End Region

#Region "WC"
F.Data.DataTable.CreateFromString("dtWC", "", "WC_Start*!*WC_End", "String*!*String", "*!*", "@!@", True)

#End Region

